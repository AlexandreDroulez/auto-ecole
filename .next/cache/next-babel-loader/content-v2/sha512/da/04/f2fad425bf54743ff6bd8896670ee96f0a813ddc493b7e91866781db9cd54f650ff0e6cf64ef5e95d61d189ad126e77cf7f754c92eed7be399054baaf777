{"ast":null,"code":"import { css } from 'styled-components';\nexport const DEFAULT_BODY_FONT_SIZE = 16;\n/**\n * @see https://getbootstrap.com/docs/4.1/layout/overview/#responsive-breakpoints\n // Small devices (landscape phones, 576px and up)\n // Medium devices (tablets, 768px and up)\n // Large devices (desktops, 992px and up)\n // Extra large devices (large desktops, 1200px and up)\n */\n\nconst sizes = {\n  phone: 576,\n  tablet: 768,\n  desktop: 992,\n  largeDesktop: 1200,\n  extraLargeDesktop: 1500\n};\nconst deviceSizes = ['phone', 'tablet', 'desktop', 'largeDesktop', 'extraLargeDesktop'];\nexport const deviceMedia = deviceSizes.reduce((acc, deviceSizeName) => {\n  acc[deviceSizeName] = (first, ...interpolations) => css([\"@media (max-width:\", \"em){\", \";}\"], sizes[deviceSizeName] / DEFAULT_BODY_FONT_SIZE, css(first, ...interpolations));\n\n  return acc;\n}, {});","map":{"version":3,"sources":["/Users/alexandredroulez/Desktop/auto-ecole/styles/helper.js"],"names":["css","DEFAULT_BODY_FONT_SIZE","sizes","phone","tablet","desktop","largeDesktop","extraLargeDesktop","deviceSizes","deviceMedia","reduce","acc","deviceSizeName","first","interpolations"],"mappings":"AAAA,SAASA,GAAT,QAAoB,mBAApB;AAEA,OAAO,MAAMC,sBAAsB,GAAG,EAA/B;AAEP;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,MAAMC,KAAK,GAAG;AACZC,EAAAA,KAAK,EAAE,GADK;AAEZC,EAAAA,MAAM,EAAE,GAFI;AAGZC,EAAAA,OAAO,EAAE,GAHG;AAIZC,EAAAA,YAAY,EAAE,IAJF;AAKZC,EAAAA,iBAAiB,EAAE;AALP,CAAd;AAQA,MAAMC,WAAW,GAAG,CAAC,OAAD,EAAU,QAAV,EAAoB,SAApB,EAA+B,cAA/B,EAA+C,mBAA/C,CAApB;AAEA,OAAO,MAAMC,WAAW,GAAGD,WAAW,CAACE,MAAZ,CAAmB,CAACC,GAAD,EAAMC,cAAN,KAAyB;AACrED,EAAAA,GAAG,CAACC,cAAD,CAAH,GAAsB,CAACC,KAAD,EAAQ,GAAGC,cAAX,KAA8Bd,GAA9B,uCACCE,KAAK,CAACU,cAAD,CAAL,GAAwBX,sBADzB,EAEhBD,GAAG,CAACa,KAAD,EAAQ,GAAGC,cAAX,CAFa,CAAtB;;AAKA,SAAOH,GAAP;AACD,CAP0B,EAOxB,EAPwB,CAApB","sourcesContent":["import { css } from 'styled-components'\n\nexport const DEFAULT_BODY_FONT_SIZE = 16\n\n/**\n * @see https://getbootstrap.com/docs/4.1/layout/overview/#responsive-breakpoints\n // Small devices (landscape phones, 576px and up)\n // Medium devices (tablets, 768px and up)\n // Large devices (desktops, 992px and up)\n // Extra large devices (large desktops, 1200px and up)\n */\n\nconst sizes = {\n  phone: 576,\n  tablet: 768,\n  desktop: 992,\n  largeDesktop: 1200,\n  extraLargeDesktop: 1500\n}\n\nconst deviceSizes = ['phone', 'tablet', 'desktop', 'largeDesktop', 'extraLargeDesktop']\n\nexport const deviceMedia = deviceSizes.reduce((acc, deviceSizeName) => {\n  acc[deviceSizeName] = (first, ...interpolations) => css`\n    @media (max-width: ${sizes[deviceSizeName] / DEFAULT_BODY_FONT_SIZE}em) {\n      ${css(first, ...interpolations)};\n    }\n  `\n  return acc\n}, {})\n"]},"metadata":{},"sourceType":"module"}